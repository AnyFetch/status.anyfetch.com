<!doctype html>
<html>
<head>
    <title>Status Fetch v1.0</title>
    <link rel="stylesheet" href="https://ajax.googleapis.com/ajax/libs/jqueryui/1.11.2/themes/smoothness/jquery-ui.css"/>
    <link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/css/bootstrap.min.css"/>
    <link rel='stylesheet' href='css/index.css'>
</head>
<body class="custom-body">
<div class='container-fluid'>
        <div class="jumbotron text-center" style='padding-bottom: 7%'>
            <h1 class="page-title">AnyFetch <%= env %> Status</h1>
            <div class="col-md-4">
                <p>Number of updates stored: <span id="slider-value"></span></p>
                <div id="slider"></div>
            </div>
            <div class="col-md-4">
                <p>Change layout
                    <p id="grid-mode"><span class="glyphicon glyphicon-th-large"></span></p>
                    <p id="justify-mode"><span class="glyphicon glyphicon-align-justify"></span></p>
                </p>
            </div>
        </div>
        <div class="jumbotron" id='critical-status'></div>
        <div class="row" id="graph-container"></div>
    </div>
</body>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
<script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.11.2/jquery-ui.min.js"></script>
<script src="https://cdn.socket.io/socket.io-1.3.3.js"></script>
<script src="components/flot/jquery.flot.js"></script>
<script src="components/flot/jquery.flot.time.js"></script>
<script src="components/flot-axislabels/jquery.flot.axislabels.js"></script>
<script src="js/fetchData.js"></script>
<script src="js/graph.js"></script>
<script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/js/bootstrap.min.js"></script>
</html>
